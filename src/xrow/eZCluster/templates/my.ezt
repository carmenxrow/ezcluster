{use $datadir}
[mysqld]
datadir={$datadir}
collation-server = utf8_unicode_ci
character-set-server = utf8

{use $settings}

{foreach $settings as $setting}
{$setting->key} = {$setting->value}
{/foreach}

[mysqld_safe]
collation-server = utf8_unicode_ci
character-set-server = utf8

[client]
default-character-set=utf8

[mysql]
default-character-set=utf8