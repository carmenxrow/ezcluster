{use $datadir}
[mysqld]
datadir={$datadir}
socket=/var/lib/mysql/mysql.sock
user=mysql
collation-server = utf8_unicode_ci
character-set-server = utf8

{use $settings}

{foreach $settings as $setting}
{$setting->key} = {$setting->value}
{/foreach}


[mysqld_safe]
log-error=/var/log/mysqld.log
collation-server = utf8_unicode_ci
character-set-server = utf8

[client]
default-character-set=utf8

[mysql]
default-character-set=utf8